// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id Int @id @default(autoincrement())
  title String
  description String
  status Boolean @default(false)

  @@map("tasks")
}

